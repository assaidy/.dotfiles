super + Return
	$TERMINAL

super + space
	rofi -show run -matching fuzzy -sorting-method normal -sort

super + b
    zen-browser

super + n
	thunar

super + Escape
	$HOME/.dotfiles/scripts/dm-power

XF86AudioLowerVolume
	amixer set Master 5%-
XF86AudioRaiseVolume
	amixer set Master 5%+
XF86AudioMute
    amixer set Master toggle

XF86MonBrightnessUp
	light -A 10%
XF86MonBrightnessDown
	light -U 10%

super + p
    maim --select | xclip -selection clipboard -t image/png && xclip -selection clipboard -o >> ~/Pictures/Screenshots/$(date +%Y-%m-%d-%s).png 
super + shift + p
	maim | xclip -selection clipboard -t image/png && xclip -selection clipboard -o >> ~/Pictures/Screenshots/$(date +%s).png 

super + c
    xcolor | xclip -selection clipboard
